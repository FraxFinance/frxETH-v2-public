{
  "printWidth": 120,
  "useTabs": false,
  "plugins": ["prettier-plugin-solidity"],
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "parser": "solidity-parse",
        "bracketSpacing": true,
        "tabWidth": 4,
        "singleQuote": false,
        "compiler": ">=0.8.0"
      }
    }
  ]
}
